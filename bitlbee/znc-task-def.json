{
    "family": "mod-services",
    "volumes": [
        {
            "name": "znc-data",
            "host": {
                "sourcePath": "/data/znc"
            }
        }
    ],
    "containerDefinitions": [
        {
            "environment": [],
            "name": "znc",
            "image": "jaelsasser/znc",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 6667,
                    "hostPort": 6667
                },
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "znc-data",
                    "containerPath": "/data"
                }
            ],
            "logConfiugration": [
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "awslogs-irc",
                    "awslogs-region": "us-east-1"
                }
            ],
            "essential": true
        }
    ]
}
